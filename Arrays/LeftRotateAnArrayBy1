// Program to Left rotate an array by 1

public class Main
{
  public static void main (String[]args)
  {
    int[] arr = new int[]{ 1, 2, 3, 4, 5 };
    int n = arr.length;
    int start = arr[0];
    for (int i = 1; i < n; i++)
      {
	arr[i - 1] = arr[i];
      }
    arr[n - 1] = start;
  for (int i:arr)
      {
	System.out.print (i + ",");
      }
  }
}
